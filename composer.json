{
    "version": "1.0.0",
    "name": "ricardominze/swoolebootstrap",
    "require": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.7",
        "phpunit/phpunit": "^11.5",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ricardominze",
            "email": "ricardominze@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0"
    }
}