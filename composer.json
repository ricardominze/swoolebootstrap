{
    "version": "1.0.0",
    "name": "ricardominze/swoolebootstrap",
    "require": {
        "phpstan/phpstan": "^2.1",
        "pestphp/pest": "^3.7",
        "phpunit/phpunit": "^11.5",
        "vlucas/phpdotenv": "^5.6",
        "open-telemetry/opentelemetry": "^1.0",
        "guzzlehttp/promises": "^2.0",
        "php-http/httplug": "^2.4",
        "monolog/monolog": "^3.8",
        "open-telemetry/opentelemetry-logger-monolog": "^1.0",
        "open-telemetry/context-swoole": "^1.0",
        "open-telemetry/exporter-otlp": "^1.2",
        "guzzlehttp/guzzle": "^7.9",
        "open-telemetry/sdk": "^1.2",
        "promphp/prometheus_client_php": "^2.13"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "ricardominze",
            "email": "ricardominze@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    },
    "require-dev": {
        "swoole/ide-helper": "^6.0"
    }
}
